#import <Foundation/Foundation.h>

@interface HMPlug : NSObject

//智能插座

@property (nonatomic, assign) NSInteger powerSwitch;  //继电器开关
@property (nonatomic, assign) NSInteger powerwf; //使能位BIT7；WFu,bit0-bit6为周一至周日bit7 定时器使能定时器单次功能与周期功能是二选一，bit0-bit6都是0为单次，否则为周期，周期月、日无效
@property (nonatomic, assign) NSInteger powersDay; //开始日
@property (nonatomic, assign) NSInteger powersMonth; //开始月
@property (nonatomic, assign) NSInteger powersHour; //开始时
@property (nonatomic, assign) NSInteger powersMinutes;//开始秒
@property (nonatomic, assign) NSInteger powereDay; //结束日
@property (nonatomic, assign) NSInteger powereMonth; //结束月
@property (nonatomic, assign) NSInteger powereHour; //结束时
@property (nonatomic, assign) NSInteger powereMinutes;//结束秒


@property (nonatomic, assign) NSInteger usbSwitch;//USB 开关
@property (nonatomic, assign) NSInteger usbwf;//使能位BIT7；WFu,bit0-bit6为周一至周日bit7 定时器使能定时器单次功能与周期功能是二选一，bit0-bit6都是0为单次，否则为周期，周期月、日无效
@property (nonatomic, assign) NSInteger usbsDay; //开始日
@property (nonatomic, assign) NSInteger usbsMonth; //开始月
@property (nonatomic, assign) NSInteger usbsHour; //开始时
@property (nonatomic, assign) NSInteger usbsMinutes;//开始秒
@property (nonatomic, assign) NSInteger usbeDay; //结束日
@property (nonatomic, assign) NSInteger usbeMonth; //结束月
@property (nonatomic, assign) NSInteger usbeHour; //结束时
@property (nonatomic, assign) NSInteger usbeMinutes;//结束秒

@end
