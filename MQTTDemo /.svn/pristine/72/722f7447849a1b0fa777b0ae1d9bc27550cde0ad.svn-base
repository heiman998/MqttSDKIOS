#import <Foundation/Foundation.h>

@interface HMThp : NSObject

//温湿度设置

@property (nonatomic, assign)NSInteger temUp; //温度阈值上限;
@property (nonatomic, assign)NSInteger temLow; //温度阈值上限;
@property (nonatomic, assign)NSInteger humUp; //湿度阈值上限;
@property (nonatomic, assign)NSInteger humLow; //湿度阈值上限;

@property (nonatomic, assign)Boolean humEnbale; //湿度使能 0为不使能 1为使能 其他值无效
@property (nonatomic, assign)Boolean tempEnbale; //温度使能 0为不使能 1为使能 其他值无效

@end
